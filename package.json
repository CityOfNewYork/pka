{
  "name": "pka",
  "version": "2.0.0",
  "description": "Pre-K Finder",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "watch": "webpack --watch",
    "build": "webpack",
    "prebuild": "yarn roll-css",
    "roll-css": "./node_modules/nyc-lib/css/roll-css.sh ./src/css/pka.theme.css ./src/css/pka.css",
    "predeploy": "yarn build",
    "deploy": "node deploy"
  },
  "repository": "https://github.com/CityOfNewYork/pka.git",
  "author": "cmenedes <cmenedes@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.3.1",
    "nyc-lib": "^1.3.52",
    "ol": "^5.3.0",
    "papaparse": "^4.5.1",
    "proj4": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "canvas-prebuilt": "1.6.11",
    "coveralls": "^3.0.2",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^1.6.5",
    "nyc-build-helper": "^0.0.32",
    "webpack": "4.19.1",
    "webpack-cli": "2.0.14"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFiles": [
      "./__tests__/jest-setup.js"
    ],
    "transformIgnorePatterns": [],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/js/**/*.js"
    ]
  }
}
